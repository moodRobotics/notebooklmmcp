{
  "name": "notebook-mcp-server",
  "version": "1.0.0",
  "description": "Node.js Model Context Protocol server for Google NotebookLM",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "docs"
  ],
  "bin": {
    "notebook-mcp-server": "dist/index.js",
    "notebook-mcp-auth": "dist/auth-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "auth": "ts-node src/auth-cli.ts",
    "example:workflow": "ts-node examples/workflow.ts",
    "example:local": "ts-node examples/local-files.ts",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "axios": "^1.13.4",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "ora": "^5.4.1",
    "playwright": "^1.58.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.4",
    "@types/node": "^20.19.32",
    "@types/ora": "^3.2.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/uuid": "^9.0.8",
    "pdf-parse": "^2.4.5",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "notebooklm",
    "google",
    "ai",
    "server"
  ],
  "author": "Antigravity",
  "license": "MIT"
}